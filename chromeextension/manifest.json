{
    "version": "1",
    "name": "DocuFlow",
    "description": "Helps you document a process",
    "host_permissions": ["<all_urls>"],
    "background":{
        "service_worker": "bg.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": [],
            "js": ["contentScript.js"]
        }
    ],
    "icons":{
        "128": "ico.png"
    },
    "permissions": [
        "storage",
        "tabs",
        "cookies",
        "activeTab",
        "nativeMessaging",
        "browsingData"
    ],
    "action":{
        "default_icon":{
            "128": "ico.png"
        },
        "default_title": "DocuFlow - Documentations made easier",
        "default_popup": "docuFlow.html"
    },
    "minimum_chrome_version": "92",
    "manifest_version": 3
}